{
    "name": "brianfp/laravel-firebase-auth",
    "description": "Secure your laravel API with Google Firebase Auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rui Sardinha",
            "email": "mail@ruisardinha.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3.0",
        "kreait/firebase-tokens": "1.11",
        "illuminate/support": "^8.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "csrui\\LaravelFirebaseAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "csrui\\LaravelFirebaseAuth\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "csrui\\LaravelFirebaseAuth\\FirebaseAuthServiceProvider"
            ]
        }
    }
}
